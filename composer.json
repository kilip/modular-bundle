{
    "name": "doyo/modular-bundle",
    "description": "Symfony Modular Bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Doyo\\Bundle\\Modular\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Doyo\\Bundle\\Modular\\": "tests/",
            "spec\\Doyo\\Bundle\\Modular\\": "spec/",
            "App\\": "tests/sandbox/src"
        }
    },
    "authors": [
        {
            "name": "Anthonius Munthi",
            "email": "me@itstoni.com"
        }
    ],
    "config": {
        "sort-packages": true,
        "preferred-install": {
            "*": "dist"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "doctrine/inflector": "^2.0",
        "symfony/framework-bundle": "^5.3",
        "symfony/serializer": "^5.3"
    },
    "require-dev": {
        "api-platform/core": "^2.6",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/mongodb-odm-bundle": "^4.3",
        "doctrine/orm": "^2.9",
        "friends-of-phpspec/phpspec-code-coverage": "^6.1",
        "friendsofphp/php-cs-fixer": "^3.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^0.12.98",
        "phpstan/phpstan-phpunit": "^0.12.22",
        "phpstan/phpstan-symfony": "^0.12.44",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.16.1",
        "psalm/plugin-symfony": "^3.0",
        "symfony/dotenv": "^5.3",
        "symfony/property-access": "^5.3",
        "symfony/property-info": "^5.3",
        "symfony/runtime": "^5.3",
        "symfony/serializer-pack": "^1.0",
        "symfony/validator": "^5.3",
        "vimeo/psalm": "^4.10"
    }
}
